@model NationalIT.TempReport
@using Webdiyer.WebControls.Mvc;
@{
    ViewBag.Title = "Driver's Payrolls";
}
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <a href="#" class="btn btn-info btnRePrint">RePrint</a> <a href="@Url.Action("RollBack", "DriverInfo")" class="btn btn-info btnRollBack">
                RollBack</a> <a href="@Url.Action("Index", "DriverInfo")" class="btn">Back</a>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <select data-placeholder="Select Date" name="Date" class="chosen-with-diselect-date input-xlarge">
                @Html.Raw(ViewBag.dataDate)
            </select>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12 wrap-ALlPartial">
            @Html.Partial("_IndexUndoPayrollPartial", Model)
        </div>
    </div>
</div>
