@model Alabama.CreatedCard

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Details</title>
</head>
<body>
    <fieldset>
        <legend>CreatedCard</legend>
    
        <div class="display-label">UserID</div>
        <div class="display-field">@Model.UserID</div>
    
        <div class="display-label">Amount</div>
        <div class="display-field">@Model.Amount</div>
    
        <div class="display-label">PriceLevel</div>
        <div class="display-field">@Model.PriceLevel</div>
    
        <div class="display-label">DateCreate</div>
        <div class="display-field">@String.Format("{0:g}", Model.DateCreate)</div>
    
        <div class="display-label">DateRelease</div>
        <div class="display-field">@String.Format("{0:g}", Model.DateRelease)</div>
    
        <div class="display-label">DateExpire</div>
        <div class="display-field">@String.Format("{0:g}", Model.DateExpire)</div>
    
        <div class="display-label">Note</div>
        <div class="display-field">@Model.Note</div>
    </fieldset>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>

