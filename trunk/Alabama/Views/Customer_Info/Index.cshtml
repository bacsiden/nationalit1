@model IEnumerable<Alabama.Customer_Info>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row-fluid">
    <div class="span12">
        <div class="box">
            <div class="box-title">
                <h3>
                    <i class="icon-table"></i>Danh sách cước phí</h3>
            </div>
            <div class="box-content">
                <p style="margin-left: 7px;">
                    <img src="/Content/img/add.png" width="14" height="14" style="padding-bottom: 2px;" />
                    @Html.ActionLink("Create New", "Create", null, new { @class = "btnThemMoi" })
                </p>
                <table class="table">
                    <thead>
                        <tr class="table-flag-blue">
                            <th>
                            </th>
                            <th>
                                City
                            </th>
                            <th>
                                State
                            </th>
                            <th>
                                ZIP_Code
                            </th>
                            <th>
                                Phone
                            </th>
                            <th>
                                Fax
                            </th>
                            <th>
                                Contact
                            </th>
                            <th>
                                Comments
                            </th>
                            <th>
                                Days_to_pay
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                    @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ }, new { @class = "confirmDelete" })
                                </td>
                                <td>
                                    @item.City
                                </td>
                                <td>
                                    @item.State
                                </td>
                                <td>
                                    @item.ZIP_Code
                                </td>
                                <td>
                                    @item.Phone
                                </td>
                                <td>
                                    @item.Fax
                                </td>
                                <td>
                                    @item.Contact
                                </td>
                                <td>
                                    @item.Comments
                                </td>
                                <td>
                                    @item.Days_to_pay
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
