@model List<NationalIT.TempReport>
<h3>
    List pay rolls rollback</h3>
<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Amount
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.ActionLink(item.Date.ToString(), "PayrollsRollBack", "DriverInfo", new { driverID = item.DriverID, id = item.ID }, null)
                </td>
                <td>
                    Amount
                </td>
            </tr>
            }
        }
    </tbody>
</table>
</div> 